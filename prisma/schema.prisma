generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Challenge {
  id           Int    @id @default(autoincrement())
  url          String @unique
  waldo_top    Int
  waldo_left   Int
  waldo_bottom Int
  waldo_right  Int
}

model User {
  id    Int     @id @default(autoincrement())
  name  String? @unique
  score Int
}
